library verilog;
use verilog.vl_types.all;
entity stopwatch is
    port(
        clk             : in     vl_logic;
        rst             : in     vl_logic;
        sout            : out    vl_logic_vector(7 downto 0);
        mout            : out    vl_logic_vector(7 downto 0);
        hout            : out    vl_logic_vector(7 downto 0)
    );
end stopwatch;
